{
  "name": "@pivotal-flow/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./security/password": {
      "import": "./dist/security/password.js",
      "types": "./dist/security/password.d.ts"
    },
    "./security/jwt-types": {
      "import": "./dist/security/jwt-types.js",
      "types": "./dist/security/jwt-types.d.ts"
    },
    "./tenancy/guard": {
      "import": "./dist/tenancy/guard.js",
      "types": "./dist/tenancy/guard.d.ts"
    },
    "./types/user": {
      "import": "./dist/types/user.js",
      "types": "./dist/types/user.d.ts"
    },
    "./pricing": {
      "import": "./dist/pricing/index.js",
      "types": "./dist/pricing/index.d.ts"
    },
    "./metrics": {
      "import": "./dist/metrics/index.js",
      "types": "./dist/metrics/index.d.ts"
    },
    "./redis": {
      "import": "./dist/redis.js",
      "types": "./dist/redis.d.ts"
    },
    "./metrics/quote-metrics": {
      "import": "./dist/metrics/quote-metrics.js",
      "types": "./dist/metrics/quote-metrics.d.ts"
    },
    "./guards/jsonbMonetaryGuard": {
      "import": "./dist/guards/jsonbMonetaryGuard.js",
      "types": "./dist/guards/jsonbMonetaryGuard.d.ts"
    },
    "./db/filterGuard": {
      "import": "./dist/db/filterGuard.js",
      "types": "./dist/db/filterGuard.d.ts"
    },
    "./cache/types": {
      "import": "./dist/cache/types.js",
      "types": "./dist/cache/types.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "dependencies": {
    "argon2": "^0.40.0",
    "decimal.js": "^10.4.3",
    "drizzle-orm": "^0.44.5",
    "ioredis": "^5.3.2",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/decimal.js": "^7.4.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}