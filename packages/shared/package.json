{
  "name": "@pivotal-flow/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./security/password": {
      "import": "./dist/security/password.js",
      "types": "./dist/security/password.d.ts"
    },
    "./security/jwt-types": {
      "import": "./dist/security/jwt-types.js",
      "types": "./dist/security/jwt-types.d.ts"
    },
    "./tenancy/guard": {
      "import": "./dist/tenancy/guard.js",
      "types": "./dist/tenancy/guard.d.ts"
    },
    "./types/user": {
      "import": "./dist/types/user.js",
      "types": "./dist/types/user.d.ts"
    },
    "./db/repo.users": {
      "import": "./dist/db/repo.users.js",
      "types": "./dist/db/repo.users.d.ts"
    },
    "./metrics": {
      "import": "./dist/metrics/index.js",
      "types": "./dist/metrics/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "argon2": "^0.40.0",
    "ioredis": "^5.3.2",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0"
  }
}