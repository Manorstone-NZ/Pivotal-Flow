{
  "name": "@pivotal-flow/sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "TypeScript SDK for Pivotal Flow API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react-query": {
      "import": "./dist/react-query.mjs",
      "require": "./dist/react-query.js",
      "types": "./dist/react-query.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "generate": "tsx scripts/generate.ts",
    "generate:watch": "tsx scripts/generate.ts --watch",
    "prepublishOnly": "npm run clean && npm run build && npm run type-check",
    "publish:dry-run": "npm pack --dry-run",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "pivotal-flow",
    "api",
    "sdk",
    "typescript",
    "react-query"
  ],
  "author": "Pivotal Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Manorstone-NZ/Pivotal-Flow.git",
    "directory": "packages/sdk"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
